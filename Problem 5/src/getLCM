package getLCM;

public class Main {
	private static int gcd(int a, int b) {
		if (a < 0 || b < 0) throw new IllegalArgumentException();
		while (b > 0) {
			int temp = b;
			b = a % b;
			a = temp;
		}
		return a;
	}
	private static int getLCM(int a, int b) {
		if (a < 0 || b < 0) throw new IllegalArgumentException();
		return (a * b) / gcd(a, b);
	}
	public static void main(String[] args) {
		int n = 20, result = 1;
		for (int i = 2; i < n; i++) {
			result = getLCM(result, i);
		}
		System.out.println(result);
	}
}
