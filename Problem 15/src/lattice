package lattice;

public class Main {
	private static long binomial(int n, int k) {
		long[][] binomial = new long[n+1][k+1];
		for (int i = 0; i <= n; i++)
			binomial[i][0] = 1;
		
		for (int i = 1; i <= n; i++) {
			for (int j = 1; j <= k; j++) {
				binomial[i][j] = binomial[i-1][j-1] + binomial[i-1][j];
			}
		}
		return binomial[n][k];
	}
	public static void main(String[] args) {
		System.out.println(binomial(40, 20));
	}
}
